<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.READ_CALENDAR" />
    <uses-permission android:name="android.permission.WRITE_CALENDAR" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.KotAndroid">
        <activity
            android:name=".ui.BtmActivity"
            android:exported="false"
            android:label="@string/title_activity_btm">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".ui.WebActivity"
            android:exported="false" />
        <activity
            android:name=".ui.FingerActivity"
            android:exported="false" />
        <activity
            android:name=".ui.PanActivity"
            android:exported="false" />
        <activity
            android:name=".ui.MarqueeActivity"
            android:exported="false" />
        <activity
            android:name=".ui.LineChartActivity"
            android:exported="false" />
        <activity
            android:name=".ui.CalenderActivity"
            android:exported="false" />
        <activity
            android:name=".TouchActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>